{
  "name": "LubnaDev Theme",
  "type": "dark",
  "colors": {
    "focusBorder": "#773f8175",
    "editor.background": "#141318",
    "editor.foreground": "#ffffff90",
    "editor.findMatchBackground": "#773f8175",
    "editor.findMatchHighlightBackground": "#01b8cb25",
    "editor.lineHighlightBackground": "#ffffff06",
    "editor.selectionBackground": "#ffffff25",
    "menubar.selectionBackground": "#512b57",
    "menu.background": "#1d1e22",
    "menu.foreground": "#d3d6dc",
    "editorCursor.foreground": "#d0b5d5",
    "editorGroupHeader.tabsBackground": "#17181c",
    "editorGroupHeader.tabsBorder": "#141318",
    "editorLineNumber.foreground": "#ffffff25",
    "editorLineNumber.activeForeground": "#ffffff75",
    "editorIndentGuide.background": "#ffffff08",
    "editorIndentGuide.activeBackground": "#83458d90",
    "editorSuggestWidget.background": "#17181b",
    "editorSuggestWidget.border": "#17181b",
    "editorSuggestWidget.selectedBackground": "#512b5775",
    "editorSuggestWidget.highlightForeground": "#e95384",
    "editorWidget.background":"#17181c",
		"editorWidget.border":"#512b57",
    "button.background": "#83458d",
    "button.foreground": "#ffffff",
    "textLink.foreground": "#22c0f0",
    "dropdown.background": "#23252a",
    "input.background": "#23252a50",
    "input.border": "#141318",
    "list.activeSelectionBackground": "#512b57",
    "list.activeSelectionForeground": "#d7dae0",
    "list.hoverBackground": "#ffffff10",
    "list.inactiveSelectionBackground": "#ffffff20",
    "sideBar.background": "#17181c",
    "sideBarSectionHeader.background": "#00000015",
    "statusBar.background": "#623869",
    "statusBar.foreground": "#d5c2d8",
    "tab.activeBackground": "#141318",
    "tab.border": "#141318",
    "tab.inactiveBackground": "#17181c",
    "titleBar.activeBackground": "#17181c",
    "titleBar.activeForeground": "#d3d6dc",
    "activityBarBadge.background": "#e95384",
    "activityBarBadge.foreground": "#e9e2e4",
    "activityBar.background": "#141318",
    "activityBar.foreground": "#b68fbc"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffffff50"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.template"],
      "settings": {
        "foreground": "#b186b7"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#ba6bc6"
      }
    },
    {
      "name": "Embedded String Begin/End",
      "scope": [
        "string.embedded.begin",
        "string.embedded.end",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#e9799d"
      }
    },
    {
      "name": "Types",
      "scope": "storage.type",
      "settings": {
        "foreground": "#e9799d"
      }
    },
    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#e95384"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor"
      ],
      "settings": {
        "foreground": "#e95384"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#e95384"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#e95384"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": {
        "foreground": "#11c2d3"
      }
    },
    {
      "name": "Variable Object",
      "scope": [
        "variable.other.object",
        "variable.other.constant",
        "variable.other.global",
        "variable.other.readwrite.class",
        "variable.other.readwrite.instance",
        "variable.other.readwrite.batchfile",
        "variable.readwrite",
        "variable.readwrite.other.block"
      ],
      "settings": {
        "foreground": "#11c2d3"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#92bd3a"
      }
    },
    {
      "name": "Package Import",
      "scope": ["storage.modifier.import", "storage.modifier.package"],
      "settings": {
        "foreground": "#11c2d3"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#92bd3a"
      }
    },
    {
      "name": "Builtin Functions",
      "scope": ["function.support.builtin", "function.support.core"],
      "settings": {
        "foreground": "#92bd3a"
      }
    },
    {
      "name": "Tag Name",
      "scope": ["entity.name.tag", "entity.name.tag.class.js"],
      "settings": {
        "foreground": "#e95384"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#92bd3a"
      }
    },
    {
      "name": "StyleSheet Property value",
      "scope": [
        "support.constant.css",
        "support.constant.scss",
        "support.constant.less",
        "support.constant.sass"
      ],
      "settings": {
        "foreground": "#92bd3a"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#e95384"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#92bd3a"
      }
    },
    {
      "name": "Link",
      "scope": "string.detected-link",
      "settings": {
        "foreground": "#11c2d3"
      }
    },
    {
      "name": "Markdown: Link",
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#11c2d3"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e95384"
      }
    },
    {
      "name": "Markup: Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#92bd3a"
      }
    },
    {
      "name": "Default Embedded Color",
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.begin.erb",
        "punctuation.section.embedded.end.erb",
        "source.elixir.embedded",
        "punctuation.separator",
        "punctuation.accessor",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#01b8cb"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#e9799d"
      }
    },
    {
      "name": "Other variable",
      "scope": [
        "variable.other",
        "variable.other.property",
        "variable.other.block"
      ],
      "settings": {
        "foreground": "#c6c6c7"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter",
        "entity.name.variable.parameter",
        "parameter.variable"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c5a9ca"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#c5a9ca"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.variable"],
      "settings": {
        "foreground": "#92bd3a"
      }
    },
    //undone
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#e2a122"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function-call",
      "settings": {
        "foreground": "#92bd3a"
      }
    },
    {
      "name": "Tag Class and ID",
      "scope": ["entity.name.tag.class", "entity.name.tag.id"],
      "settings": {
        "foreground": "#e95384"
      }
    },
    {
      "name": "StyleSheet Property name",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.less",
        "support.type.property-name.sass"
      ],
      "settings": {
        "foreground": "#bbbcbd"
      }
    },
    {
      "name": "CSS: Pseudo Attribute Names",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-class.scss",
        "entity.other.attribute-name.pseudo-class.less",
        "entity.other.attribute-name.pseudo-class.sass",
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-element.scss",
        "entity.other.attribute-name.pseudo-element.less",
        "entity.other.attribute-name.pseudo-element.sass"
      ],
      "settings": {
        "foreground": "#b186b7"
      }
    },
    {
      "name": "StyleSheet Variable",
      "scope": [
        "variable.css",
        "variable.scss",
        "variable.less",
        "variable.sass"
      ],
      "settings": {
        "foreground": "#3ccad7"
      }
    },
    {
      "name": "StyleSheet Variable String",
      "scope": [
        "variable.css.string",
        "variable.scss.string",
        "variable.less.string",
        "variable.sass.string"
      ],
      "settings": {
        "foreground": "#c5a9ca"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#c6c6c7"
      }
    },

    {
      "name": "JSON String",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#bbbbbb"
      }
    },

    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic, markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Punctuation",
      "scope": [
        "punctuation.definition.italic.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#f1f1f1"
      }
    },
    {
      "name": "Markup: Emphasis Punctuation",
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}
